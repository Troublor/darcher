{
  "name": "@darcher/examples",
  "version": "1.0.0",
  "repository": {
    "url": "https://github.com/Troublor/darcher"
  },
  "author": "Troublor",
  "main": "src",
  "license": "ISC",
  "scripts": {
    "deploy:giveth": "ts-node ./giveth/scripts/deploy.ts",
    "start:giveth": "ts-node ./giveth/scripts/start.ts",

    "deply:augur": "cd augurproject/scripts && bash ./deploy.sh",
    "start:augur-blockchain": "ts-node augurproject/scripts/start_augur_blockchain.ts",
    "docker:build:augur-blockchain": "cd augurproject/scripts && bash ./docker_build_augur-base.sh",
    "docker:start:augur-blockchain": "docker-compose -f augurproject/scripts/docker/docker-compose.yml up",
    "docker:stop:augur-blockchain": "docker-compose -f augurproject/scripts/docker/docker-compose.yml down"
  },
  "dependencies": {
    "ts-node": "^8.10.2",
    "typescript": "^3.9.6"
  }
}
